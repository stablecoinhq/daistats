{
  "name": "daistats",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://daistats.com",
  "dependencies": {
    "defi18n": "../defi18n/",
    "defi18n": "https://github.com/stablecoinhq/defi18n.git#492fe9191e01e600b6ca6d4944ccda606e720e5c",
    "ethers": "^5.0.31",
    "gh-pages": "^3.1.0",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "next": "^13.0.5",
    "node-polyglot": "^2.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-polyglot": "^0.7.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.3",
    "react-tabs": "^3.2.2",
    "recharts": "^2.0.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.34.0",
    "eslint": "6.6.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "12.2.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-i18next": "^6.0.0-6",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-unused-imports": "^2.0.0",
    "prettier": "^2.7.1",
    "sass": "^1.54.5",
    "stylelint": "^14.11.0",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.3.0",
    "svgo": "^2.8.0"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start:goerli": "REACT_APP_NETWORK=goerli react-scripts start",
    "start:mainnet": "REACT_APP_NETWORK=mainnet react-scripts start",
    "start": "REACT_APP_NETWORK=mainnet react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "yarn lint:es && yarn lint:style && yarn lint:prettier",
    "lint:fix": "yarn -s lint:es:fix; yarn -s lint:style:fix; yarn -s lint:prettier:fix; yarn -s svgo",
    "lint:es": "eslint '**/*.{js,jsx,ts,tsx}'",
    "lint:es:fix": "eslint --fix '**/*.{js,jsx,ts,tsx}'",
    "lint:prettier": "prettier --check '**/*.{js,jsx,ts,tsx}'",
    "lint:prettier:fix": "prettier -w '**/*.{js,jsx,ts,tsx}'",
    "lint:style": "stylelint 'src/**/*.{css,less,sass,scss}'",
    "lint:style:fix": "stylelint --fix 'src/**/*.{css,less,sass,scss}'",
    "svgo": "svgo -r src"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
